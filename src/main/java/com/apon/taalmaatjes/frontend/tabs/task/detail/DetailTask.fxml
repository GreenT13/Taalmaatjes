<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<!--suppress ALL -->

<HBox prefHeight="346.0" prefWidth="456.0" stylesheets="@DetailTask.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apon.taalmaatjes.frontend.tabs.task.detail.DetailTask">
    <children>
        <VBox prefHeight="311.0" prefWidth="543.0" HBox.hgrow="ALWAYS">
            <children>
                <HBox fx:id="hboxError">
                    <children>
                        <Label id="labelError" fx:id="labelError" text="Label" />
                    </children>
                </HBox>
                <VBox>
                    <children>
                        <HBox>
                            <children>
                                <Hyperlink graphicTextGap="0.0" prefHeight="0.0" prefWidth="0.0" />
                                <TextField fx:id="labelTitle" editable="false">
                                    <styleClass>
                                        <String fx:value="copyable-label" />
                                        <String fx:value="largeFont" />
                                    </styleClass></TextField>
                                <Hyperlink onAction="#goToScreenEditTask" styleClass="largeFont" text="(bewerken)" />
                                <Region prefHeight="1.0" prefWidth="50.0" HBox.hgrow="ALWAYS" />
                                <Button mnemonicParsing="false" onAction="#goBack" text="Terug" />
                            </children>
                        </HBox>
                        <HBox>
                            <children>
                                <VBox prefHeight="42.0" prefWidth="176.0">
                                    <children>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Vrijwilliger" />
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                                <VBox>
                                    <children>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <TextField fx:id="labelVolunteer" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                                    <opaqueInsets>
                                                        <Insets />
                                                    </opaqueInsets>
                                                </TextField>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                  <TextArea fx:id="textAreaDescription" editable="false" prefHeight="261.0" prefWidth="538.0" wrapText="true" />
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

</HBox>
