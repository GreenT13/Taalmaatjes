<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox stylesheets="@DetailVolunteer.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apon.taalmaatjes.frontend.tabs.volunteers.detail.DetailVolunteer">
    <children>
        <VBox HBox.hgrow="ALWAYS">
            <children>
                <VBox>
                    <children>
                        <HBox>
                            <children>
                                <TextField fx:id="labelName" editable="false">
                           <styleClass>
                              <String fx:value="copyable-label" />
                              <String fx:value="largeFont" />
                           </styleClass></TextField>
                                <Region prefHeight="1.0" prefWidth="50.0" HBox.hgrow="ALWAYS" />
                                <Button mnemonicParsing="false" onAction="#back" text="Terug" />
                            </children>
                        </HBox>
                        <HBox>
                            <children>
                                <VBox prefHeight="42.0" prefWidth="176.0">
                                    <children>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Geboortedatum" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Telefoon nummer" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Mobiele telefoonnr" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Email" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Straat" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Postcode" />
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                                <Label text="Stad" />
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                                <VBox>
                                    <children>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelDateOfBirth" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelPhoneNr" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelMobPhoneNr" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelEmail" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelStreetNameAndHouseNr" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelPostalCode" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                        <HBox styleClass="hboxAlign">
                                            <children>
                                    <TextField fx:id="labelCity" editable="false" prefHeight="21.0" styleClass="copyable-label">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                    </TextField>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox prefWidth="100.0">
                    <children>
                        <Label styleClass="largeFont" text="Actief" />
                        <VBox fx:id="vboxActive" />
                    </children>
                    <padding>
                        <Insets top="10.0" />
                    </padding>
                </VBox>
                <VBox prefWidth="100.0">
                   <children>
                      <Label styleClass="largeFont" text="Koppelingen" />
                      <VBox fx:id="vboxMatch" />
                   </children>
                   <padding>
                      <Insets top="10.0" />
                   </padding>
                </VBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

</HBox>
